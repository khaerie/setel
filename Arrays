
public class Array {

	public Array() {
		// TODO Auto-generated constructor stub
	}

		 public static void main(String[] args) {
		  
		   // create an array
		   int[] age = {1, 2, 3, 4, 5, 6};

		   // access each array elements
		   System.out.println("Accessing Elements of Array:");
		   System.out.println("First Element: " + age[1]);
		   System.out.println("Second Element: " + age[2]);
		   System.out.println("Third Element: " + age[3]);
		   System.out.println("Fourth Element: " + age[4]);
		   System.out.println("Fifth Element: " + age[5]);
		   }
	}

